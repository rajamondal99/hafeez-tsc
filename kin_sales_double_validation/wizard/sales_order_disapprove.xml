        <record id="confirm_sale_order_disapprove_view" model="ir.ui.view">
            <field name="name">Sales Orders Disapproved</field>
            <field name="model">sale.order.disapprove.wizard</field>
            <field name="arch" type="xml">
               <form string="Withold Sales Order">
                    <p>You are about to withhold this sales order. </p>
                    <group>
                        <field name="msg"/>
                    </group>
                    <footer>
                        <button name="disapprove_sales_orders" string="Withhold Sales Order(s)" type="object" class="oe_highlight"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

