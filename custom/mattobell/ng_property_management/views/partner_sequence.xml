<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <record id="seq_supplier" model="ir.sequence">
            <field name="name">res.partner</field>
            <field name="code">res.partner</field>
            <field name="prefix">ADSUP/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_customer" model="ir.sequence">
            <field name="name">res.partner.customer</field>
            <field name="code">res.partner.customer</field>
            <field name="prefix">ADCUM/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_agent" model="ir.sequence">
            <field name="name">res.partner.agent</field>
            <field name="code">res.partner.agent</field>
            <field name="prefix">AG/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>
