<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- Sequences for Generating Zone Code -->
        <record id="seq_zone" model="ir.sequence">
            <field name="name">zone.code</field>
            <field name="code">zone.code</field>
            <field name="prefix">ADZ</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Sequences for Generating Branch Code -->
        <record id="seq_branch" model="ir.sequence">
            <field name="name">branch.code</field>
            <field name="code">branch.code</field>
            <field name="prefix">ADB</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
