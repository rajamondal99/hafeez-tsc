<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- Sequences for Generating property.number -->
        <record id="seq_student" model="ir.sequence">
            <field name="name">property.number</field>
            <field name="code">property.number</field>
            <field name="prefix">ADH/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
