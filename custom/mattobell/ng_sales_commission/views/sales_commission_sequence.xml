<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- Sequences for Generating Sales Commission -->
        <record id="seq_commission" model="ir.sequence">
            <field name="name">sales.commission</field>
            <field name="code">sales.commission</field>
            <field name="prefix">SC/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
