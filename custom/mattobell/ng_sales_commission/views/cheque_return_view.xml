<?xml version="1.0" encoding="UTF-8"?><openerp>    <data>        <!-- Claim Order -->        <record model="ir.ui.view" id="cheque_return_form_view">            <field name="name">cheque.return.form</field>            <field name="model">cheque.return</field>            <field name="arch" type="xml">                <form string="Returned Cheque">                    <header>                        <button string="Validate"                                name="action_validate"                                type="object"                                class="oe_highlight"                                states="draft"                                 groups="account.group_account_user"/>                         <field name="state" widget="statusbar"/>                    </header>                    <sheet>                        <group>                            <group>                                <h1>                                    <field name="name" placeholder="Name.." attrs="{'readonly': [('state', '!=', 'draft')]}"/>                                </h1>                            </group>                            <group>                                                            </group>                        </group>                      <group>                        <group>                            <field name="customer_id" attrs="{'readonly': [('state', '!=', 'draft')]}"/>                            <field name="journal_id"  domain="[('type', 'in', ('bank', 'cash'))]" attrs="{'readonly': [('state', '!=', 'draft')]}"/>                             <field name="revenue_journal_id"  domain="[('type', 'in', ('bank', 'cash'))]" attrs="{'readonly': [('state', '!=', 'draft')]}"/>                             <field name="deferred_journal_id"  domain="[('type', 'in', ('bank', 'cash'))]" attrs="{'readonly': [('state', '!=', 'draft')]}"/>                            <label for="cheque_amount"/>                            <div>                                <field name="cheque_amount" class="oe_inline" attrs="{'readonly': [('state', '!=', 'draft')]}"/>                                 <field class="oe_inline" name="currency_id" attrs="{'readonly': [('state', '!=', 'draft')]}"/>                            </div>                            <field name="return_charge" attrs="{'readonly': [('state', '!=', 'draft')]}"/>                                                     </group>                        <group>                            <field name="date" attrs="{'readonly': [('state', '!=', 'draft')]}"/>                             <field name="cheque_number" attrs="{'readonly': [('state', '!=', 'draft')]}"/>                             <field name="return_account_id" attrs="{'readonly': [('state', '!=', 'draft')]}"/>                             <field name="revenue_account_id" attrs="{'readonly': [('state', '!=', 'draft')]}"/>                              <field name="deferred_account_id" attrs="{'readonly': [('state', '!=', 'draft')]}"/>                             <field name="company_id" attrs="{'readonly': [('state', '!=', 'draft')]}"/>                         </group>                     </group>                     <group string="References">                         <group>                             <field name="invoice_id"/>                         </group>                         <group>                             <field name="revenue_move_id"/>                              <field name="deferred_move_id"/>                         </group>                     </group>                    </sheet>                      <div class="oe_chatter">                            <field name="message_follower_ids" widget="mail_followers" help="Follow this project to automatically track the events associated to tasks and issues of this project." groups="base.group_user"/>                            <field name="message_ids" widget="mail_thread"/>                     </div>                </form>            </field>        </record>                  <record model="ir.ui.view" id="cheque_return_tree_view">            <field name="name">cheque.return.tree</field>            <field name="model">cheque.return</field>            <field name="arch" type="xml">                <tree string="Returned Cheque">                     <field name="name"/>                     <field name="journal_id"/>                     <field name="customer_id"/>                     <field name="cheque_amount" />                </tree>            </field>        </record>                <record model="ir.ui.view" id="cheque_return_search_view">            <field name="name">cheque.return.search</field>            <field name="model">cheque.return</field>            <field name="arch" type="xml">                <search string="Returned Cheque">                     <field name="name"/>                      <field name="cheque_number"/>                     <field name="customer_id"/>                     <field name="journal_id"/>                      <field name="company_id"/>                      <field name="invoice_id"/>                      <field name="revenue_move_id"/>                     <field name="deferred_move_id"/>                     <group expand="0" string="Group By">                        <filter name="date" string="Date" context="{'group_by':'date'}"/>                         <filter name="customer_id" string="Customer" context="{'group_by':'customer_id'}"/>                         <filter name="company_id" string="Company" context="{'group_by':'company_id'}"/>                         <filter name="journal_id" string="Payment Method" context="{'group_by':'journal_id'}"/>                     </group>                </search>            </field>        </record>                <record model="ir.actions.act_window" id="action_cheque_return">            <field name="name">Returned Cheque</field>            <field name="res_model">cheque.return</field>            <field name="view_mode">tree,form</field>        </record>                <menuitem id="menu_cheque_return"                  sequence="3"                   parent="sales_commission_menu_parent_id"                  name="Returned Cheque"                   action="action_cheque_return"                  groups="base.group_configuration"/>    </data></openerp>